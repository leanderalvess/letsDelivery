{
  "name": "letsdelivery",
  "version": "1.0.0",
  "description": "Teste Desenvolvedor(a) Backend Node.JS + AWS",
  "main": "index.ts",
  "scripts": {
    "start": "clear && ts-node src/index.ts",
    "build": "clear && tsc && cp package.json dist/",
    "clearDeploy": "rm -rf dist/ && rm -rf .aws-sam/",
    "sam": "clear && npm run clearDeploy && npm run build && sam build",
    "sam:start": "npm run sam && sam local start-api",
    "sam:deploy": "npm run sam && sam deploy",
    "test": "clear && npm run clearDeploy && jest",
    "test:coverage": "clear && npm run clearDeploy && jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.731.1",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}